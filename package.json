{
   "title": "bmLaunch",
   "name": "bmlaunch",
   "version": "0.0.3",
   "description": "Your bookmarks acting as launchpad",
   "main": "index.js",
   "author": "yafp",
   "engines": {
      "firefox": ">=38.0a1",
      "fennec": ">=38.0a1"
   },
   "license": "MIT",


   "preferences": [{
      "type": "bool",
      "name": "pref_showURL",
      "value": true,
      "title": "Show URL",
      "description": "Shows URL behind the title"
   }, {
      "description": "Chars used for displaying url",
      "name": "pref_URLLength",
      "type": "integer",
      "value": 8,
      "title": "URL display length"
   }, {
      "type": "bool",
      "name": "pref_showTags",
      "value": false,
      "title": "Show Tags",
      "description": "Shows tags behind the title"
   }]
}
